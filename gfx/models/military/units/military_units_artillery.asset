entity = {
	name =		 "military_artillery_cannon_generic_entity"
	pdxmesh =	 "military_artillery_cannon_generic_mesh"

	locator = { name = "locator_decal" 
		position = { 0 0 0 } 
		rotation = { 0 0 0 } 
		scale = 1.0
	}

	attach = { locator_decal = "military_diorama_decal_6x6_entity" }

	get_state_from_parent = yes

	locator = { name = "impact_site" 	position = { 0.0 0.0 -10.0}	rotation = { 0 0 0} scale = 1.0}

	default_state = "idle"

	state = { name = "idle"
		time_offset = { 0.0 10.0 }  
		animation = 	"artillery_idle_animation"
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/anm_cannon_artillery_idle_animation" }}
	}
	
	state = { name = "defend"
		time_offset = { 0.0 10.0 }
		animation = 	"artillery_fire_defensive_animation"
		event = { time = 0.65  node = "loc_cannon" 			particle = "arms/cannon_firing"    	trigger_once = no keep_particle = no keep_sound = no sound = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/wpn_artillery_cannon_shot" }}	
		event = { time = 2.05  node = "impact_site" 		particle = "arms/cannon_explosion" 	trigger_once = no keep_particle = no keep_sound = no sound = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/wpn_artillery_cannon_impact" }}
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/anm_cannon_artillery_fire_defensive_animation" }}			
	}
	state = { name = "advance" 
		time_offset = { 0.0 10.0 }
		animation = 	"artillery_fire_aggressive_animation"
		event = { time = 0.0  node = "loc_cannon" 			particle = "arms/cannon_firing"    	trigger_once = no keep_particle = no keep_sound = no sound = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/wpn_artillery_cannon_shot" }}	
		event = { time = 1.4  node = "impact_site" 			particle = "arms/cannon_explosion" 	trigger_once = no keep_particle = no keep_sound = no sound = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/wpn_artillery_cannon_impact" }}
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/anm_cannon_artillery_fire_aggressive_animation" }}			
	}
	state = { name = "defeat" 
		animation = 	"artillery_defeat_animation"
		looping = 		no
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/weapons/artillery/cannon/anm_cannon_artillery_defeat_animation" }}
	}
}

