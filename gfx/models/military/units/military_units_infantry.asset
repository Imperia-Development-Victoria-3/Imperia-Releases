
#################################################
###       			European     			  ###
#################################################

entity = {
	name =		 "military_infantry_european_irregular_entity"
	pdxmesh =	 "military_infantry_european_t1_mesh"
	scale = 0.5
	attach = { name = weapon 		r_hand_attach = "military_arms_blunderbuss_entity" }

	get_state_from_parent = yes
	default_state = "walk"

	state = { name = "idle"  
		animation = 		"infantry_blunderbuss_idle_01_animation"
		looping = 			yes 
		propagate_state = 	{ weapon = "idle" }
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/generic/infantry/anm_infantry_blunderbuss_idle_01_animation" }}
	}

	state = { name = "walk"  
		animation = 		"infantry_blunderbuss_walk_01_animation"
		looping = 			yes 
		propagate_state = 	{ weapon = "idle" }
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/generic/infantry/anm_infantry_blunderbuss_walk_01_animation" }}
	}

	state = { name = "victory"  
		time_offset = { 0.0 10.0 }  
		animation = 		"infantry_blunderbuss_victory_01_animation"
		looping = 			yes 
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/generic/infantry/anm_infantry_blunderbuss_victory_01_animation" }}
	}


	state = { name = "defeat" 
		chance = 1
		animation = 		"infantry_blunderbuss_defeat_01_animation"
		looping = 			no 
		propagate_state = 	{ weapon = "idle" }
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/generic/infantry/anm_infantry_blunderbuss_defeat_01_animation" }}
	}

	state = { name = "defeat" 
		chance = 1
		animation = 		"infantry_blunderbuss_defeat_02_animation"
		looping = 			no 
		propagate_state = 	{ weapon = "idle" }
		event = { time = 0.0 keep_sound = no sound  = { soundeffect = "event:/SFX/Animation/generic/infantry/anm_infantry_blunderbuss_defeat_02_animation" }}
	}


	###		DEFEND STATES		###

	state = { name = "defend"
		time_offset = { 0.0 10.0 } 
		animation = 		"infantry_blunderbuss_fire_defensive_01_animation"
		looping = 			yes 
		propagate_state = 	{ weapon = "fire_defensive_01" }
	}


	###		ADVANCE STATES		###

	state = { name = "advance"
		time_offset = { 0.0 10.0 }  
		animation = 		"infantry_blunderbuss_fire_aggressive_01_animation" 
		looping = 			yes 
		propagate_state = 	{ weapon = "fire_aggressive_01" }
	}

	state = { name = "advance"
		time_offset = { 0.0 10.0 }  
		animation = 		"infantry_blunderbuss_fire_aggressive_02_animation" 
		looping = 			yes 
		propagate_state = 	{ weapon = "fire_aggressive_02" }
	}

}

